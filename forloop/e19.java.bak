class e19 
{
	public static void main(String[] args) 
	{
		int n=4;
		int a=1;
		char c='A';
		for(int i=0;i<n;i++)
		{
			for(int j=0;j<n;j++)
			{
				if(i%2==0)
				{
		System.out.print(a+" ");
		a=a+2;
				}
				else
				{
					System.out.print(c++ +" ");
				c++;
				}
			}
			System.out.println();
		}
	}
}
