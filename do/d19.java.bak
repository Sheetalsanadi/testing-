class d19 
{
	public static void main(String[] args) 
	{
		int num=6;
		int i=1;
		int sum=0;
		do
		{
			if(num%i==0)
			{
				sum=sum+i;
			}
			i++;
		}
		while(i<=num/2);
		if(sum==num)
		{
		System.out.println(num+" is a perfect number");
		}
		else
		System.out.println(num+" is not a perfect number");
	}
}
