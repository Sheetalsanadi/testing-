class d13 
{
	public static void main(String[] args) 
	{
		int n=4;
		int count=0;
		int i=1;
		do
		{
			int a=i*i;
				if(n==a)
			{
					count++;
					break;
			}
			i++;
		}
		while (i<=n/2);
		if(count==1)
		{
		System.out.println(i+" is a square root of "+n);
		}
		else
			System.out.println(i+" is not a perfect square root of "+n);
	}
}
