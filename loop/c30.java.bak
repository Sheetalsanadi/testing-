class c30   //strong number 
{
	public static void main(String[] args) 
	{
		int num=145;
		int temp=num;
		int sum=0;
		int fact=1;
		while(num>0)
		{
			
			int rem=num%10;
			while(rem>0)
			{
				fact=fact*rem--;
			}
			sum=sum+fact;
			num=num/10;
		}
		if(temp==sum)
		{
		System.out.println(temp+" is a strong number");
		}
		else
			System.out.println(temp+" is not a strong number");
	}
}
